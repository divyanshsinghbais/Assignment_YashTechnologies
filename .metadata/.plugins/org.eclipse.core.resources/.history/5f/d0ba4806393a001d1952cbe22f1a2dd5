package com.yash.ftp.oops;

import java.time.LocalDateTime;
import java.util.Scanner;

/**
 * @author divyansh.singh
 Perform the following Inheritance map and print the details of each 
objects. 
Electronics
Mobile LCD Laptop
Electornics class :- id, semiconductorType, dateOfManufacturing.
Mobile, LCD and Laptop will inherit class electronics, you have to create 
object of Electronic class and instantiate with Mobile, LCD and Laptop 
class. And print the details accordingly
 */

class Electronics {
	private int id;
	private String semiconductorType;
	private LocalDateTime dob;

	public void show() {
		System.out.println("Electronics class");
	}
}

class Mobile extends Electronics {
	public void show() {
		System.out.println("Mobile show");
	}
}

class Lcd extends Electronics {
	public void show() {
		System.out.println("LCD show");
	}
}

class Laptop extends Electronics {
	public void show() {
		System.out.println("Laptop show");
	}
}

public class Problem8 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Electronics obj1 = new Mobile();
		Electronics obj2 = new Lcd();
		Electronics obj3 = new Laptop();

		//Electronics object referring Mobile child class
		obj1.show();
		//Electronics object referring Lcd child class
		obj2.show();
		//Electronics object referring Laptop child class
		obj3.show();

	}

}
